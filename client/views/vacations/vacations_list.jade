h1 Vacation List

table.table.table-hover
  thead
    tr
      th Title
      th Departure Date
      th Return Date
      th Departure Airport
      th Arrival Airport
        
  tbody
    tr(ng-repeat='vacation in vacations', ng-click='show(vacation)')    
      td {{vacation.title}}
      td {{vacation.departureDate | date: "yyyy-MM-dd"}}
      td {{vacation.returnDate | date: "yyyy-MM-dd"}}
      td {{vacation.departureAirport}}
      td {{vacation.arrivalAirport}}

        
